{
    "framework": "vite",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/robots.txt",
            "destination": "/robots.txt"
        },
        {
            "source": "/sitemap.xml",
            "destination": "/sitemap.xml"
        },
        {
            "source": "/favicon.avif",
            "destination": "/favicon.avif"
        },
        {
            "source": "/og-image.avif",
            "destination": "/og-image.avif"
        },
        {
            "source": "/((?!assets|robots|sitemap|favicon|og-image).*)",
            "destination": "/"
        }
    ]
}